---
import Layout from '../layouts/Layout.astro';

const apiUrl = import.meta.env.ASTRO_API_URL;
---

<Layout title="GulTix">
	<main class="flex min-h-screen flex-col items-center justify-center">
		<button class="border-4 border-black rounded-lg py-2 px-4">
		  <a href={`${apiUrl}/auth/google`}>Sign in</a>
		</button>
	  </main>
</Layout>
